<mvc:View
	controllerName="<%= props.appId %>.controller.<%= props.name %>"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="<%= props.name %>"
		showNavButton="true"
		navButtonPress="onNavBack">
		<content>
			<Text text="Welcome to <%= props.name %>"/>
		</content>
	</Page>
</mvc:View>