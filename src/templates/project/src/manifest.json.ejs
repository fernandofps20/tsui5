{
	"_version": "1.12.0",

	"sap.app": {
		"id": "<%= props.appId %>",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{app_title}}",
		"description": "{{app_description}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "<%= props.appId %>.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.100.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": false,
			"cozy": false
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "<%= props.appId %>.i18n.i18n"
				}
			}
		},
		"resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
		"routing": {
			"config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "<%= props.appId %>.view",
                "controlAggregation": "pages",
                "controlId": "app"
            },
			"routes": [
				{
					"name": "main",
					"pattern": "",
					"target": ["TargetMain"]
				}
			],
            "targets": {
				"TargetMain": {
					"viewType": "XML",
					"transition": "slide",
					"viewId": "main",
					"viewName": "Main"
				}
            }
		}
	}
}